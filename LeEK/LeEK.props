<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <Libraries>$(ProjectDir)\..\..\..\Libraries\</Libraries>
    <BoostIncludes>$(Libraries)boost_1_51_0\</BoostIncludes>
    <Libs3D>opengl32.lib</Libs3D>
    <OutputDir>$(SolutionDir)$(ConfigurationName)$(ProjectName)</OutputDir>
  </PropertyGroup>
  <PropertyGroup />
  <ItemDefinitionGroup />
  <ItemGroup>
    <BuildMacro Include="Libraries">
      <Value>$(Libraries)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="BoostIncludes">
      <Value>$(BoostIncludes)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="Libs3D">
      <Value>$(Libs3D)</Value>
    </BuildMacro>
    <BuildMacro Include="OutputDir">
      <Value>$(OutputDir)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
  </ItemGroup>
</Project>